test() {
       # FIXME: add $SNAPPY search webserver
       
       sudo $SNAPPY install com.ubuntu.snappy.xkcd-webserver

       # FIXME: sucks, needed because "systemctl start" does not
       #        wait until the service is really started
       sleep 1

       T="HTTP/1.0 200 OK"
       printf "GET / HTTP/1.0\n\n"|nc localhost 80 > xkcd.html
       test_regexp "$T" cat xkcd.html
       
}
